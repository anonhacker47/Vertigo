    "axios": "^1.13.2",
    "currency-codes": "^2.2.0",
    "currency-symbol-map": "^5.1.0",
    "echarts": "^6.0.0",
    "gsap": "^3.14.2",
    "pinia": "^3.0.4",
    "pinia-plugin-persistedstate": "^4.7.1",
    "primeicons": "^7.0.0",
    "primevue": "^4.5.3",
    "requirejs": "^2.3.8",
